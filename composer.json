{
    "name":"mascherucci/pimpyaml-container",
    "type": "library",
    "description":"A simple Pimple container configurable from yaml file, symfony's style syntax",
    "keywords":["symfony","container","yaml","pimple","container-interop"],
    "homepage": "http://www.labcode.it",
    "license": "MIT",
    "authors": [
        {
            "name": "Roberto Mascherucci",
            "email": "roberto.mascherucci@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "symfony/yaml": "3.1",
        "pimple/pimple": "3.0",
        "container-interop/container-interop": "1.1"
    },
    "require-dev": {
		"codeception/codeception": "~2.1.7",
		"codeception/aspect-mock": "~1.0.0"
    },
    "autoload": {
        "psr-4": {
            "SwissArmy\\": "src"
        }
    },
    "scripts": {
        "test": [
            "@phpunit",
            "@phpcs"
        ],
        "phpunit": "php vendor/bin/phpunit",
        "phpcs": "php vendor/bin/phpcs"
    }
}
